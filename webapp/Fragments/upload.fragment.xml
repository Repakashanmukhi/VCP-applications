<core:FragmentDefinition xmlns:unified="sap.ui.unified" xmlns:layout="sap.ui.layout" xmlns:core= "sap.ui.core" xmlns:f="sap.ui.layout.form" 
xmlns="sap.m">
<Dialog id="upload" title="Upload Excel File">
    <layout:VerticalLayout>
        <ToolbarSpacer />
        <unified:FileUploader 
            id="myFileUploader" 
            name="myFileUpload" 
            tooltip="Upload your file to the local server"  
            change="onFileChange"
            uploadComplete="handleUploadComplete"/>
    </layout:VerticalLayout>
    <footer>
        <Toolbar>
            <ToolbarSpacer/>
            <Button text="Upload File" press="ExcelUpload"/>
            <Button text="Cancel" press="close"/>
        </Toolbar>
    </footer>
</Dialog>
</core:FragmentDefinition>


			


